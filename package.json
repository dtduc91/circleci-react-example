{
  "name": "ci-react-with-circle-ci",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "make-symlink-node-module": "export RNDROOT=~/node_modules/ci-react-with-circle-ci/node_modules && mkdir -p $RNDROOT && ln -svnf $RNDROOT node_modules",
    "preinstall": "npm run make-symlink-node-module",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
